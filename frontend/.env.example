# Frontend Environment Variables
# Copy this file to .env.local for local development

# API URL - leave empty if frontend and backend are same-origin
# For local development with Vite proxy, leave empty
# For production, set to your API domain if different from frontend
VITE_API_URL=

# Inngest dev server URL (local development only)
# Only needed if using Inngest polling for job status
VITE_INNGEST_URL=http://localhost:8288/v1
